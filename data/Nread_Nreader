def test_split():
    user = dict()
    x = open('user_click_data_final.txt','r')
    for i in x:
        i = i.strip().split()
        if i[0] in user:
            user[i[0]] += 1
        else:
            user[i[0]] = 1
    return user
#calculate number of news read : number of user
def statistic():
    u = test_split()
    count = 0
    count_user = dict()
    for i,j in u.items():
        if j in count_user:
            count_user[j] += 1
        else:
            count_user[j] = 1
    for i,j in count_user.items():
        print i,j

statistic()




